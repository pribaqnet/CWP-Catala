const CheckIsValidDomain = (domain) =>{
    var re = new RegExp(/^((?:(?:(?:\w[\.\-\+]?)*)\w)+)((?:(?:(?:\w[\.\-\+]?){0,62})\w)+)\.(\w{2,6})$/);
    return domain.match(re);
}
const populateOnEdit = (edit) => {
  let regname = edit.type2;
  if(regname == 'TXT'){
    $("#namereg_" + regname).val(edit.name);
