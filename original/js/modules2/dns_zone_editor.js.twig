        if(srvtarget==''){$("#srvtarget").css("border-color", "#A80705");}
        if(srvttl==''){$("#srvttl").css("border-color", "#A80705");}
        noti_bubble('{{langmod.DNSMSJ1}}','{{langmod.DNSERROR}}','error',false,false,'3000',true);
