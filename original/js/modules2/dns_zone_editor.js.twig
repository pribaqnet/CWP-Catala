                $("#srvtarget").val('');
                $("#srvttl").val('');
                $.ajax({
                    type: "POST",
                    url: "./{{users}}/index.php?module=dns_zone_editor&acc=restbind",
                    data:"domain="+$("#domainsel").val(),
