                        if(datos.responseText==1){
                            $("#namereg").val('');
                            $("#cachereg").val('');
                            $("#valuereg").val('');
                            $("#btn-srvreg").removeClass('disabled');
                            $.ajaxSetup({ headers: { 'csrftoken' : '{{ tokenajax }}' } });
                            $.ajax({
                                type: "POST",
                                url: "./{{users}}/index.php?module=dns_zone_editor&acc=paserrecord",
                                data:"domain="+$("#domainsel").val(),
                                complete: function(datos){
