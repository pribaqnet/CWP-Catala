                    btnporcess('on','btn_save_ftp','{{ langmod.ADDFTPACCOUNT }}');
                    $("#modal-addfaccountftp").modal('hide');
                }
                if(datos.responseText=='Error'){
                    noti_bubble('{{langmod.FTPERRORADD}}','{{langmod.ERROR1}}','error',true,false,'3000',true);
