let last_view = 'list_db_view';
function changeView(view){
  $('#' + last_view).slideUp('fast', function(){
    last_view = view;
    $('#' + view).slideDown('fast');
    if(view == 'add_db_view'){
      $('#mysql_add_user').hide();
      $('#newdb').val('');
    }else if (view == 'list_db_view') {
      $('#user_to_db').html('');
      $('#add_user_content').html('');
      buttonsHandle(false);
      resetFormDB();
    }
  });
}
